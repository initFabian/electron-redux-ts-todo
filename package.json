{
    "name": "electron-redux-ts-todo",
    "version": "1.1.0",
    "main": "src/main/main.ts",
    "author": "Fabian Buentello",
    "description": "Awesome Todo App!",
    "scripts": {
        "dev": "tsc && NODE_ENV=development ./node_modules/.bin/electron .",
        "package": "tsc && NODE_ENV=production ./node_modules/.bin/electron-forge package",
        "make": "tsc && NODE_ENV=production ./node_modules/.bin/electron-forge make",
        "publish": "tsc && NODE_ENV=production ./node_modules/.bin/build --mac -p always"
    },
    "build": {
        "appId": "com.fabian.todo-app",
        "productName": "Awesome Todo",
        "copyright": "Copyright Â© 2017 Fabian Buentello",
        "mac": {
            "category": "your.app.category.type",
            "target": [
                "zip",
                "dmg"
            ]
        },
        "publish": [{
            "provider": "github",
            "owner": "initfabian",
            "repo": "electron-redux-ts-todo"
        }]
    },
    "dependencies": {
        "electron-log": "^2.2.10",
        "electron-updater": "^2.15.0",
        "menubar": "^4.1.0",
        "react-bootstrap": "^0.31.3",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "redux": "^3.7.2",
        "redux-electron-store": "^0.4.1",
        "redux-thunk": "^2.2.0",
        "typescript": "^2.5.3"
    },
    "devDependencies": {
        "@types/react": "^16.0.10",
        "@types/react-bootstrap": "^0.31.4",
        "@types/react-dom": "^16.0.1",
        "@types/react-redux": "^5.0.10",
        "@types/redux-thunk": "^2.1.0",
        "electron": "^1.7.9",
        "electron-builder": "^19.41.0",
        "electron-prebuilt-compile": "1.8.1",
        "react": "^0.14.8",
        "react-dom": "^0.14.8"
    }
}